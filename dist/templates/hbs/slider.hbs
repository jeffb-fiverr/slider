<section class="slider-container js-slider-container">
    <div class="slider js-slider {{#mobile}}isMobile{{/mobile}}" data-is-mobile="{{mobile}}">
        {{#if mobile}}
            <span class="slide-current"></span> {{textOf}} {{slides.length}}
        {{else}}
            <a href="#!" class="hidden slide-nav slide-right js-slide-right"><i class="fa fa-chevron-right"></i></a>
            <a href="#!" class="hidden slide-nav slide-left js-slide-left"><i class="fa fa-chevron-left"></i></a>
        {{/if}}


        {{#slides}}
            <div class="slide js-slide" {{#itemScopeType}}itemscope itemtype="http://schema.org/{{.}}"{{/itemScopeType}}>
                <div class="thumbnail">
                    {{#unless ../mobile}}
                        {{#if typeVideo}}
                            <a href="#!" class="fa fa-play slider-video-play js-slider-video-play js-gtm-event-auto" data-gtm-category="gig-page-old" data-gtm-action="click" data-gtm-label="gallery-video-play" data-src="{{src}}" rel="nofollow" itemprop="url"></a>
                        {{/if}}
                        {{#if typeAudio}}
                            <a href="#!" class="fa fa-play slider-audio-play js-slider-audio-play" data-src="{{src}}" itemprop="url"></a>
                        {{/if}}
                        {{#if typePdf}}
                            <a href="#" class="btn-full-screen js-gtm-event-auto" data-gtm-category="gig-page-old" data-gtm-action="click" data-gtm-label="gallery-pdf-enlarge" data-full-screen-images="{{src}}" rel="noindex, nofollow">Full Screen</a>
                        {{/if}}
                    {{/unless}}

                    {{#if thumbnail}}
                        <img src="{{thumbnail}}" alt="{{name}}" itemprop="thumbnail" width="100%" data-reload="inprogress">
                    {{else}}
                        <div class="audio-wave"></div>
                    {{/if}}
                </div>

                {{#review}}
                    <footer class="work-review js-work-review">
                        <blockquote>{{text}}</blockquote>
                        <small>{{reviewer}}</small>
                    </footer>
                {{/review}}
            </div>
        {{/slides}}
    </div>

    {{#unless mobile}}
        <div class="thumbs-container">
            <a href="#!" class="hidden thumbs-nav slide-thumbs-right js-slide-thumbs-right"><i class="fa fa-chevron-right"></i></a>
            <a href="#!" class="hidden thumbs-nav slide-thumbs-left js-slide-thumbs-left"><i class="fa fa-chevron-left"></i></a>
            <div class="thumbs js-thumbs">
                {{#slides}}
                    <div class="thumb js-thumb">
                        {{#if thumbnail}}
                            <img src="{{thumbnail}}" alt="{{name}}" />
                        {{else}}
                            <div class="thumb-placeholder {{type}}-thumb"></div>
                        {{/if}}
                    </div>
                {{/slides}}
            </div>
        </div>
    {{/unless}}
</section>


